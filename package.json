{
  "name": "@kazupon/nuxt-i18n-next",
  "version": "0.0.2",
  "description": "⚠️ This nuxt module is PoC of `@nuxtjs/i18n` next",
  "homepage": "https://github.com/kazupon/nuxt-i18n-next#readme",
  "bugs": {
    "url": "https://github.com/kazupon/nuxt-i18n-next/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/nuxt-i18n-next.git"
  },
  "license": "MIT",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "sideEffects": false,
  "exports": {
    "require": "./dist/module.js",
    "import": "./dist/module.mjs"
  },
  "main": "./dist/module.js",
  "module": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist/module.d.ts",
    "dist/module.js",
    "dist/module.mjs"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:module": "siroc build",
    "fix": "npm-run-all --parallel lint:fix format:fix",
    "format:fix": "npm-run-all --parallel \"format:prettier --write\"",
    "format:prettier": "prettier --config .prettierrc --ignore-path .prettierignore '**/*.{js,json,html}'",
    "lint": "npm-run-all --parallel lint:codes",
    "lint:codes": "eslint ./src --ext .js,.ts,.vue,.json",
    "lint:fix": "npm-run-all --parallel \"lint:codes --fix\""
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@0.10.0-27219866.bba79f4",
    "@nuxtjs/i18n": "^7.0.3",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "siroc": "^0.16.0",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
